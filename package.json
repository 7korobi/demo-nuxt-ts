{
  "name": "nuxt",
  "version": "1.0.0",
  "description": "My wondrous Nuxt.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "deploy": "run-s generate dist:brotli dist:push",
    "test": "jest",
    "g": "npm install -g npm-check-updates typesync",
    "dist:brotli": "find dist -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k",
    "dist:push": "push-dir --dir=dist --branch=master --remote=gh-pages --cleanup"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.10",
    "animejs": "^3.0.1",
    "backstopjs": "^3.8.8",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.13",
    "cross-env": "^5.2.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nuxt-ts": "^2.4.5",
    "push-dir": "^0.4.1",
    "vue-petit-store": "^0.3.6",
    "vue-property-decorator": "^7.3.0"
  },
  "devDependencies": {
    "@types/animejs": "^2.0.1",
    "@types/babel-core": "6.25.6",
    "@types/bootstrap": "^4.2.2",
    "@types/jest": "^24.0.9",
    "@types/node-sass": "^4.11.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.1.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "ts-jest": "^24.0.0",
    "vue-jest": "^3.0.3"
  }
}
